{
  "name": "moqtail-ts",
  "version": "0.1.0",
  "description": "Media Over QUIC Transport client implementation",
  "license": "MIT",
  "author": "StreamingUniversity",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint .",
    "format": "prettier --check .",
    "type-check": "npx tsc --noEmit",
    "test:run": "vitest run",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "heap-js": "^2.6.0"
  }
}
